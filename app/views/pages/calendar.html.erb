<div class="container">
  <div class="row">
    <div class="col-12">
      <h1>Calendar</h1>

      <script src='https://cdn.jsdelivr.net/npm/fullcalendar@6.1.8/index.global.min.js'></script>
      <div id="calendar" data-controller="calendar" data-calendar-target="calendar" data-calendar-value='{ "title": "my event", "duration": "02:00" }'>
        <div id='draggable-el' data-calendar-target="draggableEl">drag me</div>
      </div>
    </div>
  </div>
</div>
